#include "MorseTree.h"
#include "MorseNode.h"



//The setup function is called once at startup of the sketch
void setup()
{
	// Add your initialization code here
	Serial.begin(9600);
}



// The loop function is called in an endless loop
void loop()
{
	MorseTree tree;
	//Add your repeated code here
	String word = "+";

	 word += tree.getData("....");
	 Serial.println("getdata1 finished.");
	 word += tree.getData(".");
	 word += tree.getData(".-..");
	 word += tree.getData(".-..");
	 word += tree.getData("---");
	 word += " ";
	 word += tree.getData(".--");
	 word += tree.getData("---");
	 word += tree.getData(".-.");
	 word += tree.getData(".-..");
	 word += tree.getData("-..");

	 Serial.println(word);
}
