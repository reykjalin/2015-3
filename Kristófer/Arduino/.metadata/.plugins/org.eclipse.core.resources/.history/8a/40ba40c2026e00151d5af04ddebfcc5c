#ifndef ENUMS_H_
#define ENUMS_H_

#include <Arduino.h>

class Enums {
	public:
		enum CtrlState {
			automatic,
			manual
		};
		enum State {
			readCmd,
			cmdComplete,
			readPwd,
			readTemp,
		};

		void setState(State s);
		State getState();

	private:
		State state;
		CtrlState ctrl;
};

#endif /* ENUMS_H_ */
